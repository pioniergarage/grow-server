<html>
<head>
    <!-- Opt-Out-Cookie -->
    <script>
        // Set to the same value as the web property used on the site
        var gaProperty = 'UA-25291840-2';

        // Disable tracking if the opt-out cookie exists.
        var disableStr = 'ga-disable-' + gaProperty;
        if (document.cookie.indexOf(disableStr + '=true') > -1) {
            window[disableStr] = true;
        }

        // Opt-out function
        function gaOptout() {
            document.cookie = disableStr + '=true; expires=Thu, 31 Dec 2099 23:59:59 UTC; path=/';
            window[disableStr] = true;
        }
    </script>
    <!-- End Opt-Out-Cookie -->
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-25291840-2"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-25291840-2', { 'anonymize_ip': true });
    </script>

    <title>@ViewBag.Title - GROW by PionierGarage</title>

    <link rel="icon" href="/favicon.ico" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/css/frontend.min.css" />

    <meta charset="utf-8" />
    <meta property="og:title" content="GROW - the Student Founding Contest" />
    <meta property="og:image" content="http://grow.pioniergarage.de/img/crawler_banner.png?v=3f5b" />
    <meta property="og:site_name" content="GROW by PionierGarage" />
    <meta property="og:url" content="https://grow.pioniergarage.de" />
    <meta property="og:type" content="website" />
    <meta property="og:description" content="GROW is an 11-week long founding contest for students. Realise your ideas, advance your startup and win prizes based on your progress." />

    @RenderSection("Styles", required: false)
</head>
<body class="@((ViewBag.TransparentNavbar ?? false) ? "index" : "no-sidebar")">
    <div id="page-wrapper">

        <header id="header" class="@((ViewBag.TransparentNavbar ?? false) ? "alt" : "")">
            <h1 id="logo" style="text-transform: none;"><a href="@Url.Action("Index", "Home", new { year = "" })">GROW <span>by <span style="font-family: Philosopher; font-size: 105%">PionierGarage</span></span></a></h1>
            <nav id="nav">
                <ul>
                    <li>
                        @Html.ActionLink("Home", "Index", "Home", new { year = "" })
                    </li>
                    <li>
                        @Html.ActionLink("Program", "Program", "Home", new { year = ViewBag.LatestContestYear })
                    </li>
                    <li class="submenu">
                        <a href="javascript:void(0);">Contest</a>
                        <ul>
                            <li>@Html.ActionLink("Teams", "Teams", "Home", new { year = ViewBag.LatestContestYear })</li>
                            <li>@Html.ActionLink("Mentors", "Mentors", "Home", new { year = ViewBag.LatestContestYear })</li>
                            <li>@Html.ActionLink("Judges", "Judges", "Home", new { year = ViewBag.LatestContestYear })</li>
                        </ul>
                    </li>
		<!--
                    <li>
                        @Html.ActionLink("Photo Booth", "PhotoBooth", "Home", new { year = ViewBag.LatestContestYear })
                    </li>
		-->
                    <li>
                        @Html.ActionLink("About us", "About", "Home", new { year = ViewBag.LatestContestYear })
                    </li>
                </ul>
            </nav>
        </header>

        @RenderBody()

        <footer id="footer">
            <ul class="icons">
                <li><a href="https://pioniergarage.de" target="_blank" class="icon"> <img alt="PionierGarage Website" src="/img/icon/shortcutIcon_scaled_bw_1.png" /></a></li>
                <li><a href="mailto:grow@pioniergarage.de" target="_blank" class="icon fa-envelope-o"><span class="label">Email</span></a></li>
                <li><a href="https://www.facebook.com/Pioniergarage/" target="_blank" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
                <li><a href="https://www.instagram.com/pioniergarage/" target="_blank" class="icon fa-instagram"><span class="label">Instagram</span></a></li>
            </ul>
            <ul class="copyright">
                <li><a href="http://html5up.net" target="_blank">Design</a></li>
                <li><a href="https://pioniergarage.de/impressum" target="_blank">Imprint</a></li>
                <li><a href="https://pioniergarage.de/datenschutz" target="_blank">Privacy</a></li>
            </ul>
            <div>
                <p style="text-align: center;">
                    Made with ❤ by
                    <a href="https://pioniergarage.de" target="_blank"><img src="/img/pg_logo.png" alt="PionierGarage" style="height: 24px; vertical-align: sub; margin-bottom: -1px" /></a>
                </p>
            </div>
        </footer>
    </div>

    <script src="/js/jquery.min.js"></script>
    <script src="/js/frontend.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
